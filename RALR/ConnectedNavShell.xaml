<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RALR.ConnectedNavShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RALR"
    Shell.FlyoutBehavior="Disabled"
    Title="RALR">

    <TabBar Route="ConnectedTabBar">

        <ShellContent
        Title="Actions"
        ContentTemplate="{DataTemplate local:ActionsPage}"
        Route="ActionsPage" />

        <ShellContent
        Title="Logs"
        ContentTemplate="{DataTemplate local:LogsPage}"
        Route="LogsPage" />

    </TabBar>




</Shell>
